{
  "name": "twitswap-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@nivo/circle-packing": "^0.67.0",
    "@nivo/core": "^0.67.0",
    "@nivo/waffle": "^0.67.0",
    "@reduxjs/toolkit": "^1.5.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.1",
    "babel-plugin-root-import": "^6.6.0",
    "d3-geo-projection": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-datepicker": "^3.7.0",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-simple-maps": "^2.3.0",
    "react-tagcloud": "^2.1.0",
    "react-tweet-embed": "^1.2.2",
    "recharts": "^2.0.9",
    "styled-components": "^5.2.1",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "env NODE_ENV=development npm run watch:css && react-app-rewired start",
    "build": "env NODE_ENV=production npm run watch:css && react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "lint": "eslint src/",
    "lint-fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,md}\"",
    "watch:css": "postcss src/assets/tailwind.css -o src/assets/output.css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "babel-plugin-import": "^1.13.3",
    "customize-cra": "^1.0.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.3.1",
    "prettier": "2.2.1",
    "pretty-quick": "^3.1.0",
    "react-app-rewired": "^2.1.8",
    "tailwindcss": "^2.0.4"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  }
}
